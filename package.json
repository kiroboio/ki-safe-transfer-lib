{
  "name": "@kiroboio/safe-transfer-lib",
  "version": "2.16.0",
  "description": "Safe Transfer library",
  "author": "Dmitry Melnikov <d.dash.mv@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://kiroboio.github.io/ki-safe-transfer-lib/",
  "scripts": {
    "build": "rm -rf dist && tsc && rm -rf dist/tests && mv dist/src/* dist && rm -rf dist/src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run test && npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --coverage --unhandled-rejections=none --testTimeout=15000",
    "test:watch": "jest --coverage --watchAll --unhandled-rejections=none --testTimeout=15000",
    "test:watch:clean": "jest --watchAll --unhandled-rejections=none --testTimeout=15000",
    "test:push": "CI=true jest --unhandled-rejections=none --testTimeout=15000",
    "dev:production": "export NODE_ENV=production && ts-node-dev ./tests/client.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./tests/client.ts",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src"
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiroboio/ki-safe-transfer-lib"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/ramda": "^0.27.6",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "dotenv": "^8.2.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-markdown": "^2.0.0-alpha.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "4.5.4",
    "@feathersjs/feathers": "4.5.3",
    "@feathersjs/socketio-client": "4.5.4",
    "@types/crypto-js": "^3.1.47",
    "crypto-js": "^4.0.0",
    "multicoin-address-validator": "0.4.4",
    "ramda": "0.27.0",
    "socket.io-client": "2.3.0",
    "uuid": "8.1.0"
  }
}
